{
  "name": "studiomart-v2",
  "version": "0.1.0",
  "private": true,
  "author": "harmonics-technology",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "lint:check": "yarn lint && yarn type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "yarn up -i",
    "up-latest": "yarn up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky",
    "openapi:codegen": "openapi -i https://studiomait-staging.azurewebsites.net/swagger/v1/swagger.json -o ./src/services -c axios --useOptions"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@smastrom/react-rating": "^1.5.0",
    "@types/react-slick": "^0.23.13",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.3",
    "html-react-parser": "^5.1.7",
    "locomotive-scroll": "^5.0.0-beta.11",
    "next": "^14.1.0",
    "next-auth": "^4.24.7",
    "next-client-cookies": "^1.1.0",
    "next-pwa": "^5.6.0",
    "nextjs-toploader": "^1.6.11",
    "ng_universities": "^1.1.3",
    "openapi-typescript-codegen": "^0.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.39.5",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-loading-skeleton": "^3.4.0",
    "react-multi-date-picker": "^4.4.1",
    "react-simple-placeholder-image": "^0.1.2",
    "react-slick": "^0.30.2",
    "use-debounce": "^10.0.0",
    "yup": "^1.4.0",
    "yup-password": "^0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@commitlint/cz-commitlint": "^18.6.0",
    "@playwright/test": "^1.40.1",
    "@types/react": "^18.2.48",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.2.4",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.x.x"
  }
}
